local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()

local Window = OrionLib:MakeWindow({Name = "RedABS (Fly) | Version 1.0", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
    Name = "Home",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local Tab2 = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local Section = Tab:AddSection({
    Name = "Main"
})

local Section2 = Tab2:AddSection({
    Name = "Flying"
})

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

local flySpeed = 5

local function fly()
    while true do
        local moveDirection = Vector3.new(0, 1, 0)
        local moveVector = moveDirection * flySpeed
        humanoid:Move(moveVector)

        wait(0.1)
    end
end

humanoid.Died:Connect(function()
    flySpeed = 0
end)

OrionLib:MakeNotification({
    Name = "Welcome",
    Content = "Oh No",
    Image = "rbxassetid://4483345998",
    Time = 5
})

Tab:AddLabel("Script Create By RoyRedRed Bad Tom")

Tab:AddButton({
    Name = "Discord",
    Callback = function()
        setclipboard("https://discord.com/invite/TWs3AQdmmH")
    end    
})

Tab2:AddButton({
    Name = "Fly",
    Callback = function()
        fly()
    end    
})

Tab2:AddSlider({
    Name = "Speed",
    Min = 0,
    Max = 20,
    Default = 5,
    Color = Color3.fromRGB(255,255,255),
    Increment = 1,
    ValueName = "FlySpeed",
    Callback = function(Value)
        flySpeed = Value
    end
})    

OrionLib:Init()